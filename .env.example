# Arkturian AI API Configuration

# API Security
API_KEY=your-api-key-here

# OpenAI (ChatGPT, DALL-E, TTS)
OPENAI_API_KEY=sk-...

# Anthropic (Claude)
ANTHROPIC_API_KEY=sk-ant-...

# Google (Gemini)
GOOGLE_API_KEY=AIza...

# Replicate (Image models)
REPLICATE_API_TOKEN=r8_...

# ElevenLabs (TTS & Music)
ELEVENLABS_API_KEY=...

# Storage API (for saving generated assets)
STORAGE_API_URL=https://api-storage.arkturian.com
STORAGE_API_KEY=your-storage-api-key

# Server Config
HOST=0.0.0.0
PORT=8003
DEBUG=False
LOG_LEVEL=INFO

# CORS
ALLOWED_ORIGINS=*

# Temporary file storage
TEMP_DIR=/tmp/ai-temp
MAX_TEMP_AGE_HOURS=24
